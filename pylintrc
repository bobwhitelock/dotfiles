    missing-kwoa,
    missing-module-docstring,
    no-else-raise,
    no-else-return,
    no-member,
    no-self-argument,
    pointless-statement,
    too-few-public-methods,
    too-many-ancestors,
    too-many-instance-attributes,
    too-many-lines,
    too-many-public-methods,
